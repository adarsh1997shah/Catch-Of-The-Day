{
    "env": {
		"es2020": true,
		"browser": true,
		"node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
	],
	"parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
	],
	"settings": {
		"react": {
		  "version": "16.3.3"
		}
	},
    "rules": {
		"eqeqeq": "error"
    }
}
